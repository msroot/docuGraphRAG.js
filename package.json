{
  "name": "docugraphrag",
  "version": "1.0.2",
  "description": "A graph-based RAG document question answering system",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rm -rf dist && mkdir -p dist/src && cp -r src/*.js dist/src/ && cp index.js dist/",
    "prepublishOnly": "npm run build",
    "start": "node src/index.js",
    "dev": "nodemon src/index.js"
  },
  "keywords": [
    "pdf",
    "rag",
    "llm",
    "chat",
    "ai",
    "documents",
    "vector-database",
    "neo4j"
  ],
  "author": "Yannis Kolovos",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msroot/docuGraphRAG.js.git"
  },
  "bugs": {
    "url": "https://github.com/msroot/docuGraphRAG.js/issues"
  },
  "homepage": "https://github.com/msroot/docuGraphRAG.js#readme",
  "dependencies": {
    "@langchain/community": "^0.3.35",
    "@langchain/core": "^0.3.42",
    "@langchain/openai": "^0.4.4",
    "axios": "^1.8.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "langchain": "^0.3.19",
    "multer": "^1.4.5-lts.1",
    "neo4j-driver": "^5.28.1",
    "openai": "^4.28.0",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^3.11.174",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "peerDependencies": {
    "express": "^4.18.2"
  }
}
